{
  "name": "agent-forge",
  "version": "1.0.0",
  "description": "A platform for creating, deploying, and managing AI agents on the Solana blockchain",
  "private": true,
  "workspaces": [
    "frontend",
    "backend",
    "scripts",
    "shared"
  ],
  "scripts": {
    "init-token": "npm run init-token --workspace=scripts",
    "deploy-agent": "npm run deploy-agent --workspace=scripts",
    "dev:frontend": "npm run dev --workspace=frontend",
    "dev:backend": "npm run dev --workspace=backend",
    "dev": "bash scripts/run-dev.sh",
    "build": "npm run build --workspaces",
    "test": "npm run test --workspaces",
    "lint": "npm run lint --workspaces"
  },
  "keywords": [
    "solana",
    "web3",
    "ai",
    "agents",
    "blockchain"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/node": "^22.13.11",
    "typescript": "^5.8.2"
  }
}
